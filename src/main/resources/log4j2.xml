<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN" name="LOGGER" packages="">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout
                    pattern="%highlight{%d [%t] %-5level: %msg%n%throwable}"/>
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>